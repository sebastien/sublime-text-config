{
    "author": "Sebastien Pierre",
    "name": "FF-Cyan",
    "settings": [
        {
            "settings": {
                "background": "#2A2A2A",
                "caret": "#01a6f3",
                "foreground": "#D0D0D0",
                "invisibles": "#202020",
                "lineHighlight": "#3A3A3A",
                "selection": "#606060"
            }
        },
        {
            "scope": "gutter",
            "settings": {
                "foreground": "#FF00000"
             }
        },
        // FIXME: I'd like to have whitespace and rulers separate
        {
            "scope": "meta",
            "settings": {
                "foreground": "#A0A0A0"
             }
        },
        {
            "scope": "meta.tag",
            "settings": {
                "foreground": "#D01384"
             }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "fontStyle": "",
                "foreground": "#A0A0A0",
				"background": "#3A3A3A"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant",
            "settings": {
                "foreground": "#2192B4"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#909090"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "fontStyle": "",
                "foreground": "#4AE6E9"
            }
        },
        {
            "name": "String interpolation",
            "scope": "string source",
            "settings": {
                "fontStyle": "",
                "foreground": "#4FE3D0"
            }
        },
        {
            "scope": "storage.type, name.type",
            "settings": {
                "foreground": "#E8E5A9"
             }
        },
        {
            "name": "Preprocessor line",
            "scope": "meta.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#D01384"
            }
        },
        {
            "scope": "name",
            "settings": {
                "fontStyle" : "bold",
                "color":"white"
            }
        },
        {
            "scope": "name.tag",
            "settings": {
                "fontStyle" : "normal",
                "foreground": "#4AE6E9"
            }
        },
        {
            "name": "Other name",
            "scope": "name.other",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#FFFFFF",
                "background": "#FF472C"
            }
        }
    ],
    "uuid": "410ab317-05a6-46f1-9f9f-899bd06777be"
}
